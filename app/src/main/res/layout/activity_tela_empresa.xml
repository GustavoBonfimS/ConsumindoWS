<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F8FF"
    tools:context=".telaEmpresa">

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/text"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageEmpresa">

    </com.google.android.material.textfield.TextInputLayout>


    <TextView
        android:id="@+id/TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Endereço:"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="148dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tipo:"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="185dp" />

    <TextView
        android:id="@+id/tvTipo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="tipo do estabelcimento"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="185dp" />

    <TextView
        android:id="@+id/tvNomeEmpresa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nome da empresa"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="47dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Avaliações"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="115dp"
        tools:layout_editor_absoluteY="292dp" />

    <TextView
        android:id="@+id/tvEnderecoEmpres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="endereco da empresa"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="148dp" />
</androidx.constraintlayout.widget.ConstraintLayout>